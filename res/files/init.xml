<?xml version="1.0"?> 
<!DOCTYPE init [
	<!ELEMENT init (homePage+)>
	<!ELEMENT init (caricamento+)>
	<!ELEMENT init (title+)>
	
	<!ELEMENT homePage ANY>
	<!ELEMENT caricamento ANY>
	<!ELEMENT title ANY>
]>

<!-- File di inizio
	 homePage: id della home page (modificare il file pagine.xml di conseguenza
	 caricamento: messaggio di caricamento da mostrare
-->
<init>
	<!-- Id home page -->
	<homePage>home</homePage>
	<!-- Messaggio di caricamento -->
	<caricamento>Caricamento...</caricamento>
	<!-- Titolo -->
	<title>misTET - Power on earth</title>
</init>
