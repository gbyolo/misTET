<?xml version="1.0"?>

<!DOCTYPE modules [
    <!ELEMENT modules (module*)>

    <!ELEMENT module ANY>
    <!ATTLIST module
        name CDATA #REQUIRED
    >
]>

<!--
	Here all your modules.
	To create a new module, just enter his name here, and then create a
	new folder in /modules with the same name. The main file of the module
	you created before must have the specified name.
-->

<modules>
	<module name="security"></module>
	<module name="blog"></module>
	<module name="SyntaxHighlighter"></module>
	<module name="logger"></module>
	<module name="cooler"></module>
</modules>
