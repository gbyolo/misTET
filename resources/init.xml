<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE init [
	<!ELEMENT init (homePage+)>
	<!ELEMENT init (loadMessage+)>
	<!ELEMENT init (title+)>
	
	<!ELEMENT homePage ANY>
	<!ELEMENT loadMessage ANY>
	<!ELEMENT title ANY>
]>

<!--
	 Init file
	 homePage: specifies the id of the page you want to use as homepage
	 loadMessage: message to show while misTET is loading
	 title: title tag in head section
-->
<init>
	<homePage>home</homePage>
	<loadMessage>Loading...</loadMessage>
	<title>misTET - Power on earth</title>
</init>
